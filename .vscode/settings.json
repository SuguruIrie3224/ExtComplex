{
    "rust-analyzer.enable": true,
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.cargo.useRustfmt": true,
    "rust-analyzer.cargo.useClippy": true,
    "rust-analyzer.checkOnSave.enable": true,
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.checkOnSave.allTargets": true,
    "rust-analyzer.checkOnSave.overrideCommand": ["cargo", "check", "--all-features"],
    "rust-analyzer.inlayHints.enable": true,
    "rust-analyzer.inlayHints.chainingHints": true,
    "rust-analyzer.inlayHints.typeHints": true,
    "rust-analyzer.inlayHints.parameterHints": true,
    "rust-analyzer.inlayHints.maxLength": 25,
    "rust-analyzer.highlightingOn": true,
    "rust-analyzer.hoverActions.enable": true,
    "rust-analyzer.hoverActions.implementations": true,
    "rust-analyzer.hoverActions.runnable": true,
    "rust-analyzer.hoverActions.gotoTypeDefinition": true,
    "rust-analyzer.hoverActions.gotoDefinition": true,
    "rust-analyzer.hoverActions.gotoSymbol": true,
    "rust-analyzer.hoverActions.revealHover": true,
    "rust-analyzer.hoverActions.showReferences": true,
    "rust-analyzer.hoverActions.showLocalReferences": true,
    "rust-analyzer.hoverActions.showImplementations": true,
    "rust-analyzer.hoverActions.showTypeHierarchy": true,
    "rust-analyzer.hoverActions.showMacroExpansion": true,
    "rust-analyzer.hoverActions.showDocumentation": true,
    "rust-analyzer.hoverActions.showWorkspaceSymbols": true,
    "rust-analyzer.hoverActions.showHoverActions": true
}

